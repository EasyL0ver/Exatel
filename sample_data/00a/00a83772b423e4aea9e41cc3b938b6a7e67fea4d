SELECT p.order_id AS `order number`,
p.model AS product,
' ' AS size,
p.qty AS quantity,
o.delivery_first_name AS `customer.first_name`,
o.delivery_last_name AS `customer.last_name`,
o.delivery_company AS `shipping_address.first_name`,
' ' AS `shipping_address.last_name`,
o.delivery_street1 AS `shipping_address.address1`,
o.delivery_city AS `shipping_address.city`,
o.delivery_postal_code AS `shipping_address.zip`,
'United States' AS `shipping_address.country`,
'US' AS `shipping_address.country_code`,
'MI' AS `shipping_address.province_code`
FROM uc_order_products p, uc_orders o
WHERE (p.nid = '379064' OR p.nid = '379065')
AND p.order_id = o.order_id
AND o.order_status = 'pending'
AND o.changed < UNIX_TIMESTAMP('2018-07-24')