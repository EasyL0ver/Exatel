public bool MMFtoProject()
        {
            MemoryMappedFile mmfProject = null;
            try
            {
                mmfProject = MemoryMappedFile.OpenExisting(ProjectMMFName);
            }
            catch (Exception e)
            {
                Console.WriteLine($"\n\n --- FAILED to open Memory mapped file[{ProjectMMFName}] ---- \n\n");
                Console.WriteLine($"{e} Exception caught");
                return false;
            }

            if (mmfProject != null)
            {
                var stream = mmfProject.CreateViewStream();
                stream.Position = 0;
                var streamReader = new StreamReader(stream);

                #region Debug
                if (debugShowProjectMMF)
                {
                    ShowReaderContents(streamReader);
                    stream.Position = 0;
                }

                // current folder is assumed to be   <ProjectDir>\bin\debug
                // TODO: hard coded path for .json :: eventually this will be a bad assumption... like when it leaves the developers machine.
                if (debugWriteJSONtoDisk)
                    WriteToDisk(streamReader, @"C:\Hg\TSI\VS\OS3DEToIFC\OS3DEToIFC\Non Source\project.json");
                #endregion



                stream.Position = 0;
                string JSON = streamReader.ReadToEnd();

                Project = JsonConvert.DeserializeObject<Os3de.Project>(JSON);
            }

            return true;
        }