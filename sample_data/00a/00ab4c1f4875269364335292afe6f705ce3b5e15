f6 list = drop 1 ( take 2 (reverse list) ) !!0

f8 _ [] = []
f8 m [x] 
 | m == x = [x]
 | otherwise = [x]
f8 m [x,y]
 | m == x = [x]
 | otherwise = [x,y]
f8 m (x:xs:xss) 
 | m == x = (x:xss)
 | otherwise = f8 m (xs:xss)
 
f10 c f e 
 | c == True = f e
 | otherwise = e

f12 [] = Nothing
f12 [x] = Just x
f12 list = Just (minimum list)