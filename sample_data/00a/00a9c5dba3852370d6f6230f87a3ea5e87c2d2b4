from sklearn import svm
import numpy as np

clf = svm.SVC(C=1)

filenameo='espiral_train.csv'
filenamet='espiral_test.csv'

train=np.loadtxt(filenameo,delimiter=",")
test=np.loadtxt(filenamet,delimiter=",")


trainx = train[:,0:2]
trainy = train[:,2]

testx = test[:,0:2]
testy = test[:,2]


clf.fit(trainx,trainy)

ypredict = clf.predict(testx)

print ypredict

print testy

length = len(ypredict)
count=0

for i in range(0,length):
	if(ypredict[i]!=testy[i]):
		count=count+1.0

print count/length*100