#include <iostream>
using namespace std;

int v[ 2019 ];


int main()
{
    int n, m, pozitii[ 123 ], lungimi[ 123 ], i, k;
    cout << "n = "; cin >> n;
    for( i = 0; i < n; i = i + 1 )
        cin >> v[ i ];

    cout << endl << "Vectorul de pozitii:" << endl;
    cout << "m = "; cin >> m;
    for( i = 0; i < m; i = i + 1 )
        cin >> pozitii[ i ];

    cout << endl << "Vectorul de lungimi:" << endl;
    for( i = 0; i < m; i = i + 1 )
        cin >> lungimi[ i ];

    int mutare, x;
    for( mutare = 0; mutare < m; mutare = mutare + 1 )
    {
        x = pozitii[ mutare ];
        k = lungimi[ mutare ];
        if( mutare % 2 == 0 )
            {
                cout << "Muta Maria." << endl;
                for( i = n - 1 + k; i >= x + k; i = i - 1 )
                    v[ i ] = v[ i - k ];
                n = n + k;
            }
        else
            {
                cout << "Muta Gigel." << endl;
                for( i = x - k; i  < n - k; i = i + 1 )
                    v[ i ] = v[ i + k ];
                n = n - k;
            }
        cout << endl << "Mutarea  " << mutare + 1 << endl;

        for( i = 0; i < n; i = i + 1 )
            cout << v[ i ] << " ";
        cout << endl;
    }

    return 0;
}