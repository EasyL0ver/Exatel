private void addColonneCheckBox() {
    // Creation du systeme de selection des lignes
    this.selectionModel = new CheckBoxSelectionModel<BeanModel>();
    this.columnConfigs.add(this.selectionModel.getColumn());

    this.selectionModel.getColumn().setRenderer(new GridCellRenderer<ModelData>() {

      @Override
      public Object render(final ModelData model, final String property, final ColumnData config, final int rowIndex, final int colIndex,
          final ListStore<ModelData> store, final Grid<ModelData> grid) {
        if (((EtatReunion) ((BeanModel) model.get("etatReunion")).getBean()).getId() == EtatReunionCode.ARCHIVE_SUPPRIME
            .getValue()) {
          config.cellAttr = "rowspan='2'";
          return "<div class='x-grid3-check-col-disabled'>&#160;</div>";
        }
        config.cellAttr = "rowspan='2'";
        return "<div class='x-grid3-row-checker'>&#160;</div>";
      }
    });
  }