public class DRoot {
  public static int digital_root(int n) {
    if (n < 10) {
      return n;
    }
    int temp = n;
    int temp1;
    int sum = 0;
    do {
      temp1 = temp;
      temp = (int) (temp / 10);
      sum += temp1 - temp * 10;
    } while (temp1 != 0);
    return digital_root(sum);
  }
}