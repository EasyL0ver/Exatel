#include <iostream>
#include <string>
using namespace std;
int nr_intregi = 0;
int valoare = 0;
int main()
{
	string sir;
	cin >> sir;
	for (int i = 0; i < sir.length(); i++)
	{
		if ((int)sir[i]>48&&(int)sir[i]<58)
		{
			nr_intregi++;
		}
	}
	for (int i = 0; i < sir.length(); i++)
	{
		if ((int)sir[i]>48&&(int)sir[i]<58)
		{
			if ((int)sir[i-1]==45)
			{
				valoare-=2*((int)sir[i]-48);
			}
			valoare+=(int)sir[i]-48;
		}
	}


	cout << nr_intregi << endl;
	cout << valoare;
	return 0;
}