#include <iostream>

using namespace std;
int n,i,p,q,poz,s[1000],x[1000];

int cbin(int p,int q,int a)
{
    int mij;
    while(p<=q)
    {
        mij=(p+q)/2;
        if(a>x[mij])
            q=mij-1;
        else
            p=mij+1;
    }
    return p;
}
int main()
{
    cin>>n;
    for(i=1; i<=n; i++)
        cin>>s[i];
    p=1;
    q=1;
    x[1]=s[1];
    for(i=1; i<=n; i++)
        if((poz=cbin(p,q,s[i])==q+1))
    {
        q++;
        x[q]=s[i];
        }
        else
            x[poz]=x[i];

                   cout<<q<<endl;

                   return 0;
        }