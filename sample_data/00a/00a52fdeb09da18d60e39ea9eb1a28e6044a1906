DECLARE	@param1 date, @param2 int

declare cur CURSOR LOCAL for
    select id , cast('2018-09-11' as DATE) date from dbo.Svc_RadioPosts 

open cur 

fetch next from cur into @param1 , @param2

while @@FETCH_STATUS = 0 begin 

EXEC	@return_value = [dbo].[sp_radio_calc_avg_max]
		@param1                    
		@param2,                      
    
    
		fetch next from cur into @param1 , @param2
end
close cur
deallocate cur
GO