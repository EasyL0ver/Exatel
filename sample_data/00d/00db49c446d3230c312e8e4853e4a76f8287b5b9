#!/bin/bash

##############################
#                            #
#  X-Plane startup script    #
#  fixes cURL library issue  #
#  with Toliss' A319         #
#                            #
##############################

# use libcurl library provided by libcurl-compat
# package instead of the one provided by curl package
export LD_PRELOAD=libcurl.so.3

# start X-Plane
~/games/x-plane/X-Plane-x86_64 > /dev/null 2>&1