	function getSize()
		return tonumber(frame.kk.Text)
	end
	
	frame.Down.MouseButton1Click:Connect(function()
		setSize(getSize() - 1)
	end)
	frame.Up.MouseButton1Click:Connect(function()
		setSize(getSize() + 1)
	end)
	setSize(getSize())