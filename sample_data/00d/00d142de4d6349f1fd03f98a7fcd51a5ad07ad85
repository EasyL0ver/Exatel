Up.Name = "Up"
Up.Parent = ODerDetector
Up.BackgroundColor3 = Color3.new(1, 1, 1)
Up.BackgroundTransparency = 1
Up.Position = UDim2.new(0.112961441, 0, 0.254901975, 0)
Up.Size = UDim2.new(0, 20, 0, 21)
Up.Image = "rbxassetid://38186614"

Down.Name = "Down"
Down.Parent = ODerDetector
Down.BackgroundColor3 = Color3.new(1, 1, 1)
Down.BackgroundTransparency = 1
Down.Position = UDim2.new(0.108433738, 0, 0.539215684, 0)
Down.Rotation = 180
Down.Size = UDim2.new(0, 20, 0, 21)
Down.AutoButtonColor = false

function yayaya()
	local theGUI = game.CoreGui:FindFirstChild("RHSize")
	RHSize.Name = "RHSize"
	if theGUI == nil then return end
	local wad = theGUI:FindFirstChild("ODerDetector")
	local newPos = wad.Position
	ODerDetector.Position = newPos
	local size = wad.kk.Text
	ODerDetector.kk.Text = size
	theGUI:Destroy()
end

yayaya()

Down.Image = "rbxassetid://38186614"
-- Scripts:
	local UserInputService = game:GetService("UserInputService")
	
	local gui = ODerDetector
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
	
	
	local frame = ODerDetector
	local Event = game:GetService("ReplicatedStorage").AvatarEditor.Remote.CommitCharacterAppearance
	
	
