ODerDetector.BackgroundColor3 = Color3.new(0.0470588, 1, 0.572549)
ODerDetector.Position = UDim2.new(0, 0, 0, 0)
ODerDetector.Size = UDim2.new(0, 83, 0, 82)

kk.Name = "kk"
kk.Parent = ODerDetector
kk.BackgroundColor3 = Color3.new(0.0470588, 1, 0.572549)
