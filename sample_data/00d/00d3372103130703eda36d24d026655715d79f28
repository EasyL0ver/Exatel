#include <stdio.h>
#include <stdlib.h>

int countNumberInK(int k) {
    int n = 0;
    while(k) {
        k % 10;
        k = k / 10;
        n++;
    }
    return n;
}
int getK(){
    int k;
    scanf("%i", &k);
    return k;
}
int DigitN(int k, int n2){
    float n = 0;
    if (countNumberInK(k) < n2){
        return -1;
    } else {
        for (int i = 1; i <= n2; i++){
            n = k % 10;
            k /= 10;
        }
        return n;
    }

}
int main()
{
    int tabWithK[6];
    for (int i = 1; i <= 5; i++){
        printf("Podaj K%i: ", i);
        tabWithK[i] = getK();
    }
    int tab[5];
    printf("\n---------------------------\n");
    for (int q = 1; q <= 5; q++){
        for (int i = 1; i <= 5; i++){
            if (i == 1){
                printf("K%i:  ", q);
            }
            if (DigitN(tabWithK[q], i) == -1) {
                printf("%i   ", DigitN(tabWithK[q], i));
            } else {
                printf(" %i   ", DigitN(tabWithK[q], i));
            }
        }
        printf("\n");
    }
}