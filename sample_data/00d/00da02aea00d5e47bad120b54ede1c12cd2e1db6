int x = 8; 
int y = x; 
x = x + 8; 
What is x? What is y?

class Silly { 
    boolean hi; 

    Silly(boolean hi) { 
        this.hi = hi;
    }

    void switch() { 
        this.hi = !this.hi; 
    }

}


class ExamplesSilly { 

    s1 = new Silly(true); 
    s2 = new Silly(false); 
    s3 = s2; 
    s2 = s1; 
    s3.switch(); 
    s1.switch(); 

    //What is in s2? 



}