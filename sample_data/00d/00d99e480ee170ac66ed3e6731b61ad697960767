(a)
    def reverse_list(l):

        rlist=[]

        rlist=l[::-1]

        return rlist


    input_list=input("enter any list separated by space : ")

    original_list=input_list.split()

    print(type(original_list))

    print(reverse_list(original_list))
	
(b)
    def reverse_list(l):

        l.reverse()

        return l

    original_list=['a','b','c']

    print(reverse_list(original_list))