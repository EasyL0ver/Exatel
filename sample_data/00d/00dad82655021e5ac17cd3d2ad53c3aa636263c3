#include <bits/stdc++.h>
 
#define fr first
#define sc second
#define ll long long
#define OK puts("OK");
#define pb emplace_back
#define mk make_pair
#define sz(s) (int) s.size()
#define all(s) s.begin(), s.end()
 
using namespace std;
 
const int N = 2e5+10;
const int inf = 1e9+7;
 
map <string, int> mp, np;
string s[2000];
int n;
vector <pair <int, int> > v;
 
string get(int a) {
    string j = "";
    for(int i = n - 1; i >= 0; i--) {
        if ((1 << i) & a) {
            j += "1";
        } else {
            j += "0";
        }
    }
    return j;
}
 
int main() {
    cin >> n;
    for(int i = 0, x; i < (1 << n); i++) {
        cin >> s[i] >> x;
        np[s[i]] = x;
        if (i)
            v.pb(mk(__builtin_popcount(i), i));
    }
    sort(all(v));
    mp[get(0)] = np[get(0)];
    for(int i = 0; i < sz(v); i++) {
        int k = v[i].sc, l = v[i].sc, m = 0;
        while(1) {
            l = (l - 1) & k;
            m ^= mp[get(l)];
            if (l == 0)
                break;
        }
        mp[get(k)] = m ^ np[get(k)];
    }
    for(int i = 0; i < (1 << n) ; i++)
        cout << s[i] << " " << mp[s[i]] << endl;
}