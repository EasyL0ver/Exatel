p2:=(n1*n2)+(n0*(n0-1) div 2);
p3:=(n0*(n0-1)*(n0-2) div 6) + (n1*(n1-1)*(n1-2) div 6) + (n2*(n2-2)*(n2-1) div 6) + n0*n1*n2;
p4:=(n0*(n0-1)*(n0-2)*(n0-3) div 24) + (n0*(n1*(n1-2)*(n1-1) div 6)) + (n0*(n2*(n2-2)*(n1-1) div 6)) + (n1*n2*(n0*(n0-1) div 2)) + ((n1*(n1-1) div 2)*(n2*(n2-1) div 2));
writeln(p2);
writeln(p3);
writeln(p4);