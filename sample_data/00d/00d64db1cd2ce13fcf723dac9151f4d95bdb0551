public class N1Runner {
	public void run() {
		ExecutorService executor = Executors.newSingleThreadExecutor();
		N1Worker worker = new N1Worker();
		Future<StatisticResult> result = executor.submit(worker);
		executor.shutdown();

		printStatistic(result);
	}

	private void printStatistic(Future<StatisticResult> result) {
		try {
			System.out.println(result.get().toString());
		} catch (InterruptedException | ExecutionException e) {
		}
	}
}