class Campaign(db.Model):
    id = db.Column(db.Integer(), primary_key=True)
    user_id = db.Column(db.Integer(), ForeignKey('user_v2.id'))
    sku = db.Column(db.String())
    category = db.Column(db.String())
    campaign_type = db.Column(db.String())
    marketplace = db.Column(db.String())
    name = db.Column(db.String())
    product_url = db.Column(db.String())
    product_name = db.Column(db.String(), nullable=True)
    product_description = db.Column(db.String())
    #headline = db.Column(db.String(), nullable=True)
    #description_headline = db.Column(db.String(), nullable=True)
    price = db.Column(db.Float())
    percent_discount = db.Column(db.Float())
    #sale_price = db.Column(db.Float())
    start_date = db.Column(db.String())
    epoch_start_date = db.Column(db.Integer())
    end_date = db.Column(db.String())
    epoch_end_date = db.Column(db.Integer())
    is_show_logo = db.Column(db.Boolean())
    #logo_url = db.Column(db.String(), nullable=True)
    #is_logo_clickable = db.Column(db.Boolean())
    #logo_redirect_url = db.Column(db.String(), nullable=True)
    #navigation_link_name = db.Column(db.String(), nullable=True)
    #navigation_link_url = db.Column(db.String(), nullable=True)
    display_countdown = db.Column(db.Boolean())
    artificial_urgency = db.Column(db.Boolean())
    #array
    image_urls = db.Column(db.String())
    #array
    #video_urls = db.Column(db.String())
    #show_discount_value = db.Column(db.Boolean())
    show_coupon_left = db.Column(db.Boolean())
    artificial_scarcity = db.Column(db.Boolean())
    show_video = db.Column(db.Boolean())
    video_url = db.Column(db.String(), nullable=True)
    privacy_text = db.Column(db.String(), nullable=True)
    show_privacy_in_footer = db.Column(db.Boolean())
    show_privacy_in_optin = db.Column(db.Boolean())
    #encourage_headline = db.Column(db.String(), nullable=True)
    require_email = db.Column(db.Boolean())
    require_first_name = db.Column(db.Boolean())
    require_last_name = db.Column(db.Boolean())
    verify_email_address = db.Column(db.Boolean())
    send_follow_up_email = db.Column(db.Boolean())
    send_follow_up_email_in = db.Column(db.Integer(), nullable=True)
    facebook_pixel_tracking = db.Column(db.Boolean())
    facebook_pixel_id = db.Column(db.String(), nullable=True)
    google_analytics_tracking = db.Column(db.Boolean())
    google_analytics_id = db.Column(db.String(), nullable=True)
    redirect_on_voucher_claimed = db.Column(db.Boolean())
    voucher_claimed_redirect_url = db.Column(db.String(), nullable=True)
    #promo_code = db.Column(db.String(), nullable=True)
    total_voucher = db.Column(db.Integer(), nullable=True)
    remaining_voucher = db.Column(db.Integer(), nullable=True)
    custom_url_name = db.Column(db.String(), nullable=True)
    created_at = db.Column(db.String())
    epoch_created_at = db.Column(db.Integer())
    email_from_name = db.Column(db.String())
    email_reply_to = db.Column(db.String())
    email_footer_address = db.Column(db.String())
    email_followup_subject = db.Column(db.String())
    email_followup_headline = db.Column(db.String())
    email_followup_body = db.Column(db.String())
    email_voucher_coupon_subject = db.Column(db.String())
    email_voucher_coupon_headline = db.Column(db.String())
    email_voucher_coupon_body = db.Column(db.String())
    is_active = db.Column(db.Boolean())