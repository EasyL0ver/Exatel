import random
GuessNumber=0
GuessNum=0
Number=int(random.randint(1,20)

while GuessNum < 6:
	GuessNum=GuessNum+1
	print ('Please enter a number within 1 and 20: ');GuessNumber=int(raw_input())
	if GuessNumber < Number
		print ('Higher')
	elif GuessNumber > Number
		print ('Lower')
	elif GuessNumber == Number
		print ('Correct!')
		break
else
	if GuessNumber==Number
		print ('Correct!')
	else
		print ('Only Six Tries are Permitted)