Problem 5
#include <stdio.h>


int main(int argc,char *argv[])
{
  
  for(int i=0;i<argc;i++)
    if(argv[i][0]!='-')      
      for(int j=i+1;j<argc;j++)
		if(argv[j][0]=='-')
		{  
			char *s=*(argv+i);             
			*(argv+i)=*(argv+j);       
			*(argv+j)=s;             
			break;
        }
		
  for(int i=0;i<argc;i++)
     printf("%s \n",argv[i]);
	
  
  return 0;
}

Problem 6
#include <stdio.h>
#include <string.h>
#include <ctype.h>


void sp(char t[], char *p[100], int len)
{
	int i=0;
	while (t[i]!='\0')
		{
			if(isspace(t[i]))
			{	
				p[len]=t[i];
				len++;
			}
			i++;
		}
	p[len]='\0';
	for(int i=0;i<=len;i++)
		printf("%p\n",&p[i]);
}

int main()
{
	char t[]="ala bala portocala ",*p[100];
	int len;
	sp(t,p,0);
	
	
	
}