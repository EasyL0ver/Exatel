#include<bits/stdc++.h>
using namespace std;
map <long long int, pair<bool, pair<int, int> > > mapa;
#define maxn 1000005
bitset <maxn> sito;
vector <int> pierwsze;
long long pot[64],q,n,suma,k,dp[maxn];
char zn;
string s;

int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(0), cout.tie(0);
	cin >> n;
	for (int i=1; i<=n; i++)
	{
		cin >> s;
		dp[0]=1;
		dp[1]=2;
		for (int j=1; j<s.size(); j++)
		{
			if (s[j]!=s[j-1])
			{
				dp[j]=dp[j-1]*2;
			}
			else
			{
				for (int l=j-1; l>0; l--)
				{
					if (s[l]==s[j])
					{
						dp[j]=dp[j-1]*2 - dp[l-1];
						l=0;
					}
				}
			}
			
		}
		cout << dp[s.size()-1] << "\n";
	}
}