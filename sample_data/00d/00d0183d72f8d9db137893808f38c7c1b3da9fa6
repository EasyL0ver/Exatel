#include <stdio.h>
#include <malloc.h>
#include <stdlib.h>
#include <time.h>
int main(void)
{
int i, k, n, m, l;
int *a;
FILE *file;
a=(int*)malloc((n)* sizeof(int));
scanf("%d", &n);
printf("Choose function: 0 - txt file, 1 - random generator\n");
scanf("%d", &l);
if (l==0){
file=fopen("file.txt","r");
for(i=0; i<n; i++)
{
fscanf(file,"%d", &a[i]);
}
fclose(file);
}
if (l==1){
//	srand(time(NULL));
	for(i=0;i<n;i++){
		a[i]=rand()%100+1;
		printf("%d ", a[i]);
	}
}
m=n;
while (m != 0){
for (i=0;i<m-1;i++)
{
	if (a[i]>a[i+1])
	{
		k=a[i+1];
		a[i+1]=a[i];
		a[i]=k;
	}
}
m=m-1;
}
for (i=0; i<n; i++){
	printf("%d\n", a[i]);
}
return 0;
}