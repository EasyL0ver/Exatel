#include <stdio.h>
#include <math.h>

int main ()
{
  int v, x1, y1, x2, y2, x3, y3, i=0, t=0, l, d;
  scanf("%d", &v);
  scanf("%d %d", &x1, &y1);
  scanf("%d %d", &x2, &y2);
 
  d=( (int)sqrt( ( (x2-x1)*(x2-x1) )+( (y2-y1)*(y2-y1) ) ) );
  v=v-2;

 do
 {
  scanf("%d %d", &x3, &y3);
  d=d+( (int) (sqrt ( (x3-x2)*(x3-x2)+(y3-y2)*(y3-y2) ) ) );
  v=v-1;
  x2=x3;
  y2=y3;
 }
 while (v>0);
 
 d=d+( (int) (sqrt ( (x3-x1)*(x3-x1)+(y3-y1)*(y3-y1) ) ) );
 
 do
 {
 t=t+1;
 l=d%t;
  if (l==0);
  i=i+t;
 }
 while (t<d);
 
 if (d%i==0)
  printf("CARAIO\n");
  
  /* 
  printf("%d\n", d);
  */
 return 0;
}