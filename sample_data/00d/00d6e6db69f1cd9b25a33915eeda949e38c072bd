#include<stdio.h>
#include<math.h>
#define a (0.5)
#define b (1)
#define s (0.3)
#define h (0.13)

double min(double x1, double x2) {
    if (x1 < x2) {
        return x1;
    }
    
    return x2;
}

double max(double x1, double x2) {
    if (x1 > x2) {
        return x1;
    }
    
    return x2;
}

int main() {
    
    for (double x = -3; x <= 3; x += h) {
        printf("x = %lf", x);
        double c = exp(-a * x) * sqrt(x + 1);
        printf(" c = %lf", c);
        double d = exp(-b * x) * pow((x + 1.5), (1 / 9));
        printf(" d = %lf", d);
        double z = (b * s * s - a) / (exp(a * x) + s);
        printf(" z = %lf", z);
        
        double y;
        if (c < z) {
            y = min(c, d) / max(c, d);
        } else {
            y = max(max(c, d), z);
        }
        printf(" y = %lf\n", y);
    }
    
}