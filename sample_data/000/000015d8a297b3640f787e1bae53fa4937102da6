SELECT p_joueurs.id AS p_joueur_id, p_joueurs.nom, p_joueurs.no, p_joueurs.niveau, p_joueurs.type1, p_joueurs.type2, p_joueurs.shiney, p_joueurs.lieu, p_joueurs.id_membre, p_joueurs.eq,
   joueurs.id AS joueur_id, joueurs.pseudo, 
   connected.id 
FROM connected 

LEFT JOIN joueurs, p_joueurs 
   ON joueurs.id = connected.id
LEFT JOIN p_joueurs
   ON p_joueurs = connected.id

WHERE p_joueurs.eq = 1 
ORDER BY niveau ASC