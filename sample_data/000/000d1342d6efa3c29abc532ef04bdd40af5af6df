
public class GoldAcc extends SaveAcc {
	
	private double minbalance;
	
	public GoldAcc (int acc, double bal, double intrestrate, double minbalance) {
		super(acc, bal, intrestrate);
		this.minbalance = minbalance;
	}
	
	public void setMinBalance(double minB) {
		minbalance = minB;
	}

	public double getMinBalance() {
		return minbalance;
	}
	
	public boolean withdraw(double amount) {
		if((super.getBalance() - amount) >= this.getMinBalance()) {
		super.withdraw(amount);
		return true;
		} else
			return false;
	}
}



