#include <iostream>

using namespace std;

int main() {
    long long m, x, y, w, h;
    cin >> m >> x >> y >> w >> h;
    if ((x%m==0 && y%m==0) && ((x+w)%m==0 && (y+h)%m==0)) {
        cout << (w*h)/(m*m);
    }
    else {
        long long x1 = x;
        long long y1 = y;
        long long y2 = y+h;
        long long x3 = x+w;

        while (x1%m!=0) x1--;
        while (y1%m!=0) y1--;
        while (y2%m!=0) y2++;
        while (x3%m!=0) x3++;
        long long x4 = x3;

        cout << ((x4-x1)*(y2-y1))/(m*m);
    }
    return 0;
}