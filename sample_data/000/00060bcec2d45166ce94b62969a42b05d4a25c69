 {addpower20.%player%hannei} = 0 #-ATK%
 {identify.%player%plus} = 0
 {sell.%player%kussyonn} = 0
 {urine1.%player%gold} = 0
 {urine2.%player%gold} = 0
 {urine3.%player%gold} = 0
 {uru.%player%waittime} = 0
 {sell.%player%talktime} = 0
 
 
on rightclick on villager:
   if display name of clicked entity is "<lime>装備買取屋":
      add 1 to {sell.%player%kussyonn}
      if {rpg.%player%class} >= 0:
         if {sell.%player%kussyonn} = 1:
            set {urine1.%player%gold} to 0
            set {urine2.%player%gold} to 0
            set {urine3.%player%gold} to 0
            message "<lime>装備買取屋:&fこの武器を売りますか?" to player
            set {selltoolname.%player%} to the name of the held item
            set {selltoollore.%player%} to the lore of the held item
            if name of held item contain "&e":
               set {urine1.%player%gold} to 3
            else if name of held item contain "&d":
               set {urine1.%player%gold} to 5
            else if name of held item contain "&b":
               set {urine1.%player%gold} to 50
            else if name of held item contain "&5":
               set {urine1.%player%gold} to 100
            if name of held item contain "&a[Lv. 1]":
               set {urine2.%player%gold} to 1
            else if name of held item contain "&a[Lv. 3]":
               set {urine2.%player%gold} to 4
            else if name of held item contain "&a[Lv. 4]":
               set {urine2.%player%gold} to 6
            else if name of held item contain "&a[Lv. 5]":
               set {urine2.%player%gold} to 7
            else if name of held item contain "&a[Lv. 7]":
               set {urine2.%player%gold} to 10
            else if name of held item contain "&a[Lv. 8]":
               set {urine2.%player%gold} to 12
            else if name of held item contain "&a[Lv. 9]":
               set {urine2.%player%gold} to 14
            else if name of held item contain "&a[Lv. 10]":
               set {urine2.%player%gold} to 16
            else if name of held item contain "&a[Lv. 12]":
               set {urine2.%player%gold} to 20
            else if name of held item contain "&a[Lv. 13]":
               set {urine2.%player%gold} to 22
            else if name of held item contain "&a[Lv. 14]":
               set {urine2.%player%gold} to 24
            else if name of held item contain "&a[Lv. 15]":
               set