variables:
     {story.%player%cooldown} = 0
     {story.%player%a} = 0 #最初の老人
 
on join:
   set {story.%player%cooldown} to 0
 
command /admin-story [<player>] [<number>]:
   executable by: player
   trigger:
      if name of player is "syu_cat":
         set {story.%arg 1%a} to arg 2
         message "<light blue>Set Completed" to player
 
on damage:
   attacker is a player
   victim is a villager
   if {rpg.%attacker%admin} = 1:
      set damage to 10000
   else:
      stop
 
on rightclick on villager:
    if display name of clicked entity is "<lime>老人":
        cancel event
        if {story.%player%cooldown} = 1:
            stop
        if {story.%player%a} = 0:
            set {story.%player%cooldown} to 1
            message "&a老人&f:ごきげんよう。きみはここの新人さんかのう？わしは老人じゃ。ここに来てもう20年経つのう。ここのルールを話そう。&n迷惑行為はBAN対象じゃぞ。&fとりあえずこれをきみに授けよう。" to player
            wait 3 seconds
            give diamond named "&eMENU" to player
            message "&7初期アイテム（メニュー）を手に入れた。" to player
            add 1 to {story.%player%a}
            set {story.%player%cooldown} to 0
            stop
        if {story.%player%a} = 1:
            set {story.%player%cooldown} to 1
            message "&a老人&f:ごきげんよう。きみはさっきの新人さんかのう？さっき与えたアイテムはなくしたのか？しかたないのう。もう一回これをきみに授けよう。" to player
            wait 3 seconds
            give diamond named "&eMENU" to player
            message "&7初期アイテム（メニュー）を再び手に入れた。" to player
            add 1 to {story.%player%a}
            set {story.%player%cooldown} to 0
            stop
        if {story.%player%a} >= 2:
            set {story.%player%cooldown} to 1
            message "&a老人&f:ごきげんよう。きみはさっきの新人さんかのう？さっき与えたアイテムはなくしたのか？何回もらえば気が済むんじゃい！！" to player
            wait 1 seconds
            message "&7老人が初期アイテムを投げてきた。" to player
            wait 1 seconds
            message "&7初期アイテムをキャッチすることができた。" to player
            give diamond named "&eMENU" to player
            message "&7初期アイテム（メニュー）を再び手に入れた。" to player
            add 1 to {story.%player%a}
        if {story.%player%a} >= 100:
            message "&a老人&f:いいかげんにしてくれのう。なくす回数にも程があるじゃろう？" to player
        set {story.%player%cooldown} to 0
 
on rightclick on villager:
   if display name of clicked entity is "<lime>アイテム換金商人":
      cancel event
      add 1 to {sell.%player%permission}
      execute player command "sell"
      wait 1 tick
      set {sell.%player%permission} to 0
 
variables:
   {sell.%player%permission} = 0
   {sell.%player%rottenflesh} = 0
   {sell.%player%bone} = 0
   {sell.%player%arrow} = 0
   {sell.%player%potato} = 0
   {sell.%player%carrot} = 0
   {sell.%player%ironingot} = 0
   {sell.%player%cooldown} = 0
   {heal.%player%cooldown} = 0
   {buy.%player%cooldown} = 0
 
command /sell:
   executable by: player
   trigger:
      if {sell.%player%permission} = 0:
         stop
      if {sell.%player%cooldown} = 1:
         stop
      else:
         set {sell.%player%cooldown} to 1
         set {sell.%player%gold} to 0
         set {sell.%player%rottenflesh} to 0
         set {sell.%player%bone} to 0
         set {sell.%player%arrow} to 0
         set {sell.%player%carrot} to 0
         set {sell.%player%potato} to 0
         set {sell.%player%ironingot} to 0
         loop all items in the inventory of player:
            loop 64 times:
               loop 36 times:
                  if player have 1 rotten flesh:
                     remove 1 rotten flesh from player's inventory
                     add 1 to {sell.%player%gold}
                     add 1 to {sell.%player%rottenflesh}
                  if player have 1 bone:
                     remove 1 bone from player's inventory
                     add 1 to {sell.%player%gold}
                     add 1 to {sell.%player%bone}
                  if player have 1 arrow:
                     remove 1 arrow from player's inventory
                     add 1 to {sell.%player%gold}
                     add 1 to {sell.%player%arrow}
                  if player have 1 carrot:
                     remove 1 carrot from player's inventory
                     add 10 to {sell.%player%gold}
                     add 1 to {sell.%player%carrot}
                  if player have 1 potato:
                     remove 1 potato from player's inventory
                     add 10 to {sell.%player%gold}
                     add 1 to {sell.%player%potato}
                  if player have 1 iron ingot:
                     remove 1 iron ingot from player's inventory
                     add 10 to {sell.%player%gold}
                     add 1 to {sell.%player%ironingot}
         message "&eYou have sold these of them." to player
         if {sell.%player%rottenflesh} > 0:
            message "&fRotten Flesh : %{sell.%player%rottenflesh}%" to player
         if {sell.%player%bone} > 0:
            message "&fBone : %{sell.%player%bone}%" to player
         if {sell.%player%arrow} > 0:
            message "&fArrow : %{sell.%player%arrow}%" to player
         if {sell.%player%carrot} > 0:
            message "&fCarrot : %{sell.%player%carrot}%" to player
         if {sell.%player%potato} > 0:
            message "&fPotato : %{sell.%player%potato}%" to player
         if {sell.%player%ironingot} > 0:
            message "&fIron Ingot : %{sell.%player%ironingot}%" to player
         message "&eYou got %{sell.%player%gold}% Gold." to player
         add {sell.%player%gold} to {rpg.%player%money}
         execute player command "reload-sidebar"
         set {sell.%player%cooldown} to 0
 
variables:
