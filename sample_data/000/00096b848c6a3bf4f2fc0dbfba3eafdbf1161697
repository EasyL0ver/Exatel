local stats = {"STAMINA","LUNG_CAPACITY","STRENGTH"}

for _,name in ipairs(stats) do
StatSetInt(GetHashKey('MP0_'..name), 100)
StatSetInt(GetHashKey('MP1_'..name), 100)
StatSetInt(GetHashKey('MP2_'..name), 100)
StatSetInt(GetHashKey('MP3_'..name), 100)
StatSetInt(GetHashKey('MP4_'..name), 100)
StatSetInt(GetHashKey('MP5_'..name), 100)
StatSetInt(GetHashKey('MP6_'..name), 100)
end

