def drink(): Boolean = {
    if (this.isWater && this.dangerLevel == 0) {
      this.neighbors.foreach(_.drink())
      this.isFull = false
      true
    } else if(this.isWater && this.dangerLevel != 0) {
      this.isFull = false
      true
    }
    else {
      for (booze <- this.board.boozeGlasses) {
        booze.isFull = false
      }
      false
    }
  }