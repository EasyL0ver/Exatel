function woo_add_spin() {
woocommerce_wp_select( 
array( 
    'id'      => '_select_nf',
    'label'   => __( 'Select #spins: ', 'woocommerce' ),
    'selected' => true,
    'options' => array(
        '24'   => __( '24', 'woocommerce' ),
        '36'   => __( '36', 'woocommerce' ),
        '48'   => __( '48', 'woocommerce' )
        )
    )
);