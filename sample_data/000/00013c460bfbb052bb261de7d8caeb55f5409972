var pdata = null;
$('#home_reg_frm2').submit(function(){
			if(pdata != null) return true;
			pdata = $(this).serialize();
			$.post("/reg_do.html", pdata, function(data){
				if(data.success != undefined){
					$('#frm_purchase_id').val(data.purchases_insert_id);
					$('#frm_user_id').val(data.tree_insert_id);
					$('#home_reg_frm2').submit();
					return true;
				}
			});
			return false;
		});