{% set quarter = module.quarter %}
{% set year = module.year %}
{% set date = module.date %}
{% set embargo_date = module.embargo_date %}
{% set url_prefix = module.url_prefix %}
{% set url_suffix = module.url_suffix %}
{% set queryparams = "&year__eq=" ~ year ~ "&quarter__eq=" ~ quarter %}

<!--ALABAMA-->
{# path to listing page #}
{% if request.path == "/press/category/archives/alabama/" %}
{% set table = hubdb_table_rows(table_id, "&name__contains=AL") %}

<section class="container-fluid">
  <h1 class="listing-headline">{{ quarter }}&nbsp;{{ year }}&nbsp;|&nbsp;{{ embargo_date }}</h1>
  {% for row in table %}
  <div class="row-fluid">
    <div class="listing-content">
      <ul>
        <li><a target="_blank" href="{{ url_prefix }}{{ year }}_{{ quarter }}/{{ row['embed'] }}{{ url_suffix }}">
            {{ date }} | {{ row['forecast'] }}&nbsp;{{ row['title'] }}&nbsp;{{ row['name'] }}</a></li>
      </ul>
    </div>
  </div>
  {% endfor %}
</section>

{% endif %}


