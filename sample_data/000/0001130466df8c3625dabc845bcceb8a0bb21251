set -x fish_user_paths ~/.fastlane/bin

rbenv init - | source

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/yusuke.tashiro/lib/google-cloud-sdk/path.fish.inc' ]; if type source > /dev/null; source '/Users/yusuke.tashiro/lib/google-cloud-sdk/path.fish.inc'; else; . '/Users/yusuke.tashiro/lib/google-cloud-sdk/path.fish.inc'; end; end

#go
set -x GOPATH "$HOME/go"
set -x PATH $PATH "$GOPATH/bin"

#anyenv
set -x PATH $HOME/.anyenv/bin $PATH

#ndenv
set -x NDENV_ROOT "$HOME/.anyenv/envs/ndenv"
set -x PATH $PATH "$NDENV_ROOT/bin"

set -gx PATH "$NDENV_ROOT/shims" $PATH
set -gx NDENV_SHELL fish
command ndenv rehash 2>/dev/null
function ndenv
  set command $argv[1]
  set -e argv[1]

  switch "$command"
  case rehash shell
    eval (ndenv sh-"$command" $argv|psub)
  case '*'
    command ndenv "$command" $argv
  end
end

#pyenv
set -x PYENV_ROOT "$HOME/.anyenv/envs/pyenv"
set -x PATH "$PYENV_ROOT/bin" $PATH
. (pyenv init - | psub)

set -x PYTHONPATH "/usr/local/lib/python3.6/site-packages" $PYTHONPATH 

# adb
set -gx PATH "$HOME/Library/Android/sdk/platform-tools" $PATH

# nodebrew
set -x PATH $HOME/.nodebrew/current/bin $PATH

#swiftenv
if which swiftenv > /dev/null; status --is-interactive; and source (swiftenv init -|psub); end

#direnv
eval (direnv hook fish)

#flutter
set -x PATH "$HOME/lib/flutter/bin" $PATH

#java
set -x JAVA_HOME (/usr/libexec/java_home -v "1.8")
set -x PATH "$JAVA_HOME/bin" $PATH

#android-sdk
set -x ANDROID_SDK_ROOT "/usr/local/share/android-sdk"