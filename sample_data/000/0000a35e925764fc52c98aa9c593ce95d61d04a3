#include <iostream>
#include <fstream>


int vs(FILE* f) {
ifstream ifs;
int maxNumber = -500;
int currNumber;
ifs.open(f);
if(!ifs.is_open()) {
	printf("File cant be opened\n");
	return;
}
while(!ifs.eof()) {
	ifs >> currNumber; 
	if(currNumber > maxNumber)
		maxNumber = currNumber;
}
ifs.close();
return maxNumber;
}