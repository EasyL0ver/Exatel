#include <iostream>

using namespace std;

int main() {
    double n, a, b, x, y;
    cin >> n >> a >> b >> x >> y;
    if (a > b || n < 0 || a < 0 || b < 0 || x < 0 || y < 0) {
        return 0;
    }
    else if (n <= a) {
        cout << n;
    }
    else if (n >= b) {
        cout << n*((100-y)/100);
    }
    else {
        cout << n*((100-x)/100);
    }
  return 0;
}