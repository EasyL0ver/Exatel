FRQ 1
int i=0;

boolean targetFound=false; //This will be used to stop the program from searching once the target is found.

for (i=0;i<myArray.length;i++)

{

if (myArray[i]==target)

{

int num=i;

System.out.println ("Target found at index " + num ")

boolean targetFound=true;

}//End of for loop

if (targetFound==true)

{

break; //This stops the program when the target is found.

} //End of if boolean

}

else

{

System.out.println ("Target not found;")

}

}

FRQ 2
public class BankUserAccount
{
private string name;
private  double accountBalance;

private int accountNumber;

private boolean hasGoodCredit;

public BankUserAccount()

{
name =John; //Default Contructors

accountBalance=10000.00;

accountNumber=12345;

hasGoodCredit=true;

}

public double getBalance (double accountBalance)

{

return accountBalance;

}

public void setBalance(double balanceInput)

{

//Assume we take user input for our setBalance function.

accountBalance=balanceInput; 

}