def numOfEncodings(message):
    if len(message) == 1 or len(message) == 0:
        return 1
    elif int(message[:2]) > 26:
        return numOfEncodings(message[1:])
    return numOfEncodings(message[1:]) + numOfEncodings(message[2:])

if __name__ == '__main__':
    assert numOfEncodings('111') == 3, "Given Example"
    assert numOfEncodings('456') == 1
    assert numOfEncodings('23221') == 6
    assert numOfEncodings('321') == 2