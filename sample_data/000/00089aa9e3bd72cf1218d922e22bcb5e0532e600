#include <stdio.h>
#include <stdlib.h>
#include <math.h>

void Kadane(int l, int r, int start, int i, int len, long double *array) {

    long double sum = 0, max_sum = log2(array[0]);

    for (int i = 0; i < len; i) {

        sum = sum + log2(array[i]);

        if (sum > max_sum) {

            max_sum = sum;
            l = start;
            r = i;
        }

        i++;

        if (sum < 0) {

            sum = 0;
            start = i;
        }
    }
    printf("%d %d", l, r);
}

int main() {

    int x, y, len, l = 0, r = 0, start = 0, i = 0;

    scanf("%d", &len);

    int array_a[len];
    int array_b[len];
    long double *array = (long double*)malloc(len * 2 * sizeof(long double));

    for (int i = 0; i < len; i++) {

        scanf("%d", &x);
        scanf("/");
        scanf("%d", &y);

        array_a[i] = x;
        array_b[i] = y;
    }

    for (int i = 0; i < len; i++)
        array[i] = (long double) array_a[i] / (long double) array_b[i];

    Kadane(l, r, start, i, len, array);

    free(array);

    return 0;
}