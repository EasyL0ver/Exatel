pattern = re.compile(r'Dealing (Flop|Turn|River) (?:(\w\w) (\w\w) (\w\w))$|(?:(\w\w) (\w\w) (\w\w) (\w\w))|(?:(\w\w) (\w\w) (\w\w) (\w\w) (\w\w))')
	deals = pattern.search(line)
	if(deals):
		outputstring +=("\n\n[b]" + deals.group(1) + "[/b]: ")
		string2write = ""
		for card in deals.groups():
			if card:
				if(card != deals.group(1)):
					string2write = string2write + card[0:1] + ":" + cards[card[1]] + ":, "
		outputstring +=(string2write[:-2] + "\n")