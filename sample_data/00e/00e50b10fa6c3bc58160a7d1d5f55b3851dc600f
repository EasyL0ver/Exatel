#include <iostream>
#include <vector>
#include <cstdlib>

using namespace std;

int Wypisz(vector<int> tab)
{
    for(int i=0; i<tab.size(); i++)
    {
        cout << tab[i] << endl;
    }
}

int liczbaLosowa(int lmin, int lmax)
{
    int wynik = rand()%lmax+lmin;
    return wynik;
}

vector<int> losowyVector(int dlugosc, int lmin, int lmax)
{
    vector<int> wynik(dlugosc);
    for(int i=0; i<dlugosc; i++)
    {
        wynik[i]=liczbaLosowa(lmin,lmax);
    }
    return wynik;
}

int main()
{
    vector<int> tab(4);
    tab[0]=5;
    tab[1]=3;
    tab[2]=4;
    tab[3]=8;
    cout << liczbaLosowa(-100,-50) << endl;
    Wypisz(losowyVector(4,3,5));
}