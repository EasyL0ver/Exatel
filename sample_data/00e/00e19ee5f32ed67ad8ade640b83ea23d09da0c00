select IDWOHD, * from tblwohd
select idResource, DefaultOperation, * from tblresource where IDResource = 7
select IDWODet, SeqWODet, * from TBLWODet where IDWOHD = 26 and Code = '999'


SELECT SeqWODet 
FROM TBLResource 
INNER JOIN TBLWODet AS det 
	ON det.Code = TBLResource.DefaultOperation
WHERE TBLResource.IDResource=7


select * from TBLWODet where code = '999'



SELECT SeqWODet FROM TBLResource 
INNER JOIN TBLWODet AS det 
	ON det.Code = TBLResource.DefaultOperation 	
WHERE TBLResource.IDResource=7 
AND det.IDWOHD=26