using System;

namespace Stipendiiki
{
    class Program
    {
        static void Main(string[] args)
        {
            double dohodlv = double.Parse(Console.ReadLine());
            double sredenUspeh = double.Parse(Console.ReadLine());
            double minZaplata = double.Parse(Console.ReadLine());
            double socialnaStip = (1.35 * minZaplata)-minZaplata ;
            double otilichenStip = sredenUspeh * 25;

            if (minZaplata <= dohodlv)

            {
                if (sredenUspeh < 5.50)
                {
                    Console.WriteLine("You cannot get a scholarship!");
                }
                else if (sredenUspeh >= 5.50)
                {
                    Console.WriteLine($"You get a scholarship for excellent results {Math.Floor(otilichenStip):f2} BGN");
                }

            }
            else if (minZaplata > dohodlv)
            {
                if (sredenUspeh < 4.50)
                {
                    Console.WriteLine("You cannot get a scholarship!");
                }
                else if (sredenUspeh > 4.50 && sredenUspeh < 5.50)
                {
                    Console.WriteLine($"You get a Social scholarship {Math.Floor(socialnaStip)} BGN");
                }
                else if (sredenUspeh >= 5.50 && otilichenStip >= socialnaStip)
                {
                    Console.WriteLine($"You get a scholarship for excellent results {Math.Floor(otilichenStip):f2} BGN");
                }
                else if (sredenUspeh >= 5.50 && otilichenStip < socialnaStip)
                {
                    Console.WriteLine($"You get a Social scholarship {Math.Floor(socialnaStip)} BGN");
                }
            }
              
        }
    }
}