#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

#define N 1000001


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    bool isPrime[N];
    for (long long i=0; i<N; i++)
        isPrime[i] = true;
    
    isPrime[0] = false;
    isPrime[1] = false;
    
    long long sqrt_n = ceil(sqrt(N)) + 1;
    
    for (long long i=2; i<sqrt_n; i++) {
        if (isPrime[i]) {
            for (long long j=2; j*i<N; j++)
                isPrime[j*i] = false;
        }
    }
    
    long double cum_sum[N];
    cum_sum[0] = 0;
    
    for (long long i=1; i<N; i++) {
        if (isPrime[i])
            cum_sum[i] = cum_sum[i-1] + i;
        else
            cum_sum[i] = cum_sum[i-1];
    }
    
    int t;
    cin>>t;
    
    while (t > 0) {
        
        long long n;
        cin>>n;
        
        cout<<cum_sum[n-1]<<endl;
        
        t--;
    }
    
    return 0;
}