				for i,v in pairs(cors) do
					spawn(function()
						pcall(v)
					end)
				end				
								
				newMotor(vic.Torso, Part4, CFrame.new(0,-1,-0.1), CFrame.new(0,0,0))				
				

				--Converted with ttyyuu12345's model to script plugin v4
