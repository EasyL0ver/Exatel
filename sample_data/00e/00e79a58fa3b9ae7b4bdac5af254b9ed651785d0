#include<iostream>
#include<cstring>
using namespace std;

int main()
{
    char sep[256],s[255]="a23Bw00124mcv34",*p,*q;
    int maxx=0,k=0;
    for(int i=0;i<strlen(s);i++)
    {
        if(s[i]>='a' && s[i]<='z')
            sep[k++]=s[i];
        else
            if(s[i]>='A' && s[i]<='Z')
                sep[k++]=s[i];
    }
    p=strtok(s,sep);
    while(p)
    {
        if(strlen(p)>maxx)
        {
            maxx=strlen(p);
            q=p;
        }
        p=strtok(NULL,sep);
    }
    cout<<q<<endl;
}