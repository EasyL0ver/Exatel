names = ['bobuk', 'umputun', 'ksusha', 'gray']
scores = [100500, 42, 777, 41]

list(map(print, names, scores))

# tak nado łjambda budjet każdyj raz posłjednim argumjentom,
# czto nawjernjaka njeudobno w rjeałizacii
map(tuple, names, scores)


params = (names, scores)
list(map(print, *params))

# a wot tak użje nje napiszjesz:
map(print, *params)

# A jeszczjo, curring udobnjeje, kogda łjambda pjerwym paramjetrom:

>>> from functools import partial
>>> printer = partial(map, print)

>>> list(printer(names, scores))
bobuk 100500
umputun 42
ksusha 777
gray 41
[None, None, None, None]