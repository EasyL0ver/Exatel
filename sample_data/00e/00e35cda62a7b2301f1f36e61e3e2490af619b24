#include <stdio.h>
#include <stdlib.h>


int main()
{
    int compe;  //comprimento do estacionamento
    int* vetcompe;      //vetor por metros do estacionamento
    int eventos;   //qtde entradas e saidas de veiculos
    int placa;
    int compv;
    int i;          //contador
    int j;          //contador
    int k;          //contador
    int qtde = 0;
    int guardaj = 0;
    int espaco = 0; //contador de espa√ßo dentro do estacionamento
    char evento;    //identifica entrada ou saida



    scanf("%d %d", &compe, &eventos);

    vetcompe = (int*) malloc (compe*sizeof(int));

    for(i = 0; i < eventos; i++)
    {
        vetcompe[i] = 0;     //zerar posicoes do vetor
    }

    for(i = 0; i < eventos; i++)
    {
        scanf("\n%c %d", &evento, &placa);

        if(evento == 'c')
        {
            scanf(" %d", &compv);
            for(j = 0; j < compv; j++)
            {
                if(vetcompe[j] == 0)
                {
                    if(guardaj == 0)
                        guardaj = j;
                    espaco++;
                } else
                {
                    espaco = 0;
                    guardaj = 0;
                }

                if (espaco >= compv)
                {
                    for(; guardaj <= compv;guardaj++)
                    {
                        vetcompe[guardaj] = placa;
                    }
                    qtde++;
                }


            }

        } else
        {
            for(k = 0; k < compe; k++)
            {
                if(vetcompe[k] == placa)
                    vetcompe[k]=0;
            }
        }
    }

    printf("\n\n%d\n",qtde*10);


    free(vetcompe);

    return 0;
}