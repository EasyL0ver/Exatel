command /gp [<player>] [<text>]:
    permission: admin.command.cmd
    trigger:
        if arg-1 is not set:
            message "&5================"
            message "&7/gp player Permission"
            message "&5================"
            message "&d↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓↓"
            message "&7Permission types"
            message "&bFly"
            message "&aworldedit"
			message "&aworldguard"
			message "&adefault"
        if arg 2 is "worldedit":
            execute console command "/pex user %arg 1% add worldedit.*"
            execute console command "/pex user %arg 1% remove worldedit.superpickaxe"
            execute console command "/pex user %arg 1% remove worldedit.brush"
            message "&e%arg 1%にワールドエディットの権限を渡しました"
            message "&e%player%からワールドエディットの権限をもらいました"
		if arg 2 is "worldguard":
			execute console command "/pex user %arg 1% add worldguard.*"
			message "&e%arg 1%にワールドガードの権限を渡しました"
            message "&e%player%からワールドガードの権限をもらいました"
		if arg 2 is "default"
			execute console command "/pex user %arg 1% add minecraft.command.*
			execute console command "/pex user %arg 1% remove minecraft.command.op"
			execute console command "/pex user %arg 1% remove minecraft.command.deop"
			execute console command "/pex user %arg 1% remove minecraft.command.ban"
			execute console command "/pex user %arg 1% remove minecraft.command.kick"
			execute console command "/pex user %arg 1% remove minecraft.command.ban-ip"
			execute console command "/pex user %arg 1% remove minecraft.command.pardon"
			execute console command "/pex user %arg 1% remove minecraft.command.pardon-ip"
			message "&e%arg 1%にデフォルトの権限を渡しました"
			message "&e%arg 1%からデフォルトの権限をもらいました"