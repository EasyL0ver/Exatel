import socket
server = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
server.bind(("142.44.189.41", 4511))
server.listen(99999)
print("Server on")
while True:
    con, cliente = server.accept()
    while True:
        try:
            data = con.recv(1024)
            if data == "get":
                print("Novo client!")
                f=open("Transformice.tfmdb", "r")
                l=f.read()
                f.close()
                con.send(l)
        except:break
	con.close()