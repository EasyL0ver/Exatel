const a = [{
  nazwaKampanii: "Kamapnia1",
  data: {
    tunel1: {
      clicks: 2
    }
  }
},{
  nazwaKampanii: "Kamapnia2",
  data: {
    tunel1: {
      clicks: 2
    }
  }
}]
a.foreach(d => {
  const newDocRef = db.collection(d.nazwaKampanii)
    .doc(moment().format("MM-DD-YY"))
    .set(d.data)
})