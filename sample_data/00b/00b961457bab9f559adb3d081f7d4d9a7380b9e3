select * 
from usuarios 
where created_at BETWEEN '2018-10-20' AND '2018-11-03';
	
$data = User::whereBetween('created_at', ['2018-10-20', '2018-11-03'])->get();
	
$data = DB::table('usuarios')
         ->whereBetween('created_at', ['2018-10-20', '2018-11-03'])
         ->get();
	
