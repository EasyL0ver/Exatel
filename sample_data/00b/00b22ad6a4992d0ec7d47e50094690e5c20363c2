static string Remove(string s)
    {
        s = s.Replace("-", "");
        s = s.Replace("--", "");
        s = s.Replace("+7", "");
        s = s.Replace("8", "");
        s = s.Replace("(", "");
        s = s.Replace(")", "");
        s = s.Replace(" ", "");
        if (s.Length < 7)
            s = s.Insert(0, "495");
        return s;
    }