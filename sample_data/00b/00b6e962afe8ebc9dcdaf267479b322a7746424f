##pay off debt with fixed monthly payment
def testUnpaid (b, ai):
    p = b/12
    while p % 10 != 0:
        p = round(p) // 10 * 10

    def remaining(b, ai):
        for i in range(12):
            up = b - p
            b = up + up * ai / 12
            i += 1
        return (p, up)

    remaining(b, ai)
    while remaining(b, ai)[1] >= 0:
        p = remaining(b, ai)[0] + 10
        remaining(b, ai)
    return p
    
print(testUnpaid(4773, 0.2))