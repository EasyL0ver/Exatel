def grades(name, surname, uni="Financial University", **marks):
    summ = 0
    count = 0
    print(uni, "-", name, ",", surname)
    for key,elem in marks.items():
        print(key, elem)
        count += 1
        summ += int(elem)
    print("AVERAGE=", summ/count)


grades("Meow", "Meow", Math=5, Geometry=6)