#include <stdio.h>


int main()
{

	int i, j, k;
	int n[12];
	int fatorial = 1;

	i = 0;
	j = 0;

	while (n[i] != -1)
	{
		scanf("%i", &n[i]);
		i = i + 1;
	}
	


	while (n[j] > 0)
	{
		for (k = 0; k <= n[k]; ++k)
		{
			fatorial = fatorial*k;
		}

		j = j+1;
	}

	printf("%i\n", fatorial);

	return 0;
}