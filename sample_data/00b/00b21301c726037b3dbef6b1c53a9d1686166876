public String getSessionlogin(long nomina)
{
    String sessionId = "";
    String sql = "select sessionid from empleados where num_emp=?";
    call = new SimpleJdbcTemplate(dataSource);
    try
    {
        sessionId = (String)call.queryForObject(sql,  String.class, new Object[] {
            Long.valueOf(nomina)
        });
    }
    catch(DataAccessException dataAccessException)
    {
        dataAccessException.printStackTrace();
        sessionId = "";
    }
    //I need to modify this return
    return sessionId;
}