void foo() {

}

template<int arg>
void bar() {
    foo(arg);
}


int main() {

}
	
template <typename T> void foo()
{
  bar(5);
}

void bar(int) {}

int main()
{
  foo<double>();
}
	
void bar(char) {}

template <typename T>
void foo()
{
  bar(5);
}

void bar(int) {}

int main()
{
  foo<double>();
}