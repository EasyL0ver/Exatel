function (res) {   
                    var lcHeaders = res.headers();

                    var file = new Blob([res.data], {
                        type: lcHeaders["content-type"]
                    });
                    var fileURL = URL.createObjectURL(file);
                    var a = document.createElement("a");
                    a.href = fileURL;
                    a.target = "_blank";

                    var lcFileName = lcHeaders["content-disposition"];
                    lcFileName = lcFileName.split("filename=")[1];
                    lcFileName = lcFileName.replace(new RegExp("\"", "g"), "");

                    a.download = lcFileName
                    document.body.appendChild(a);
                    a.click();
                }