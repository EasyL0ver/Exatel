if (wheel.rotation != targetRotation)
{
	Quaternion rotation = wheel.rotation;
	wheel.rotation = Quaternion.Lerp(wheel.rotation, targetRotation, Time.deltaTime * 7f);
	float num = Quaternion.Angle(rotation, wheel.rotation);
	angleRotated += num;
	clientSpinSpeed = Mathf.Lerp(clientSpinSpeed, num, Time.deltaTime * 5f);
}
else
{
	clientSpinSpeed = Mathf.Lerp(clientSpinSpeed, 0f, Time.deltaTime * 5f);
}
if (clientSpinSpeed < 0.01f)
{
	clientSpinSpeed = 0f;
}