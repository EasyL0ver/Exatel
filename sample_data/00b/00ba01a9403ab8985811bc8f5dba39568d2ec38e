find -name 'myfile.*' |& grep -v "Permission denied" 
     
find -name 'myfile.*' | grep -v "Permission denied" 
     
find -name 'myfile.*' 2> /dev/null 
     
find -name 'myfile.*' 2> /tmp/errorlog 
     
(find | egrep ^[RS].[0-9]+/.svg] | xargs head -1 )  2> /dev/null 
     
find | egrep ^[RS].[0-9]+/.svg] | xargs head -1 2> /dev/null 
     
find . -name 'myfile.*' -print 2>/dev/null