pub enum Degrees {
    ToC { value: f64 },
    ToF { value: f64 },
    Other
}

pub trait Converter {
    fn convert(self) -> f64;
}

impl Converter for Degrees::ToC {
    fn convert(self) -> f64 {
        (self.value - 32.0) / 1.8
    }
}

impl Converter for Degrees::ToF {
    fn convert(self) -> f64 {
        self.value * 1.8 + 32.0
    }
}