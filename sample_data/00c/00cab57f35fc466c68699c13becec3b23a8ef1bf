#include <stdlib.h>
#include <search.h>


int cmpint(const void *n1, const void *n2)
{
  return (*(int *)n1 - *(int *)n2);
}

int main()
{
  int nmemb = 10;

  int *arr = malloc(10 * sizeof(int));
  for (int i=0; i < 10; ++i)
    arr[i] = 200 - i * 5;

  int *found;
  for (int i=150; i < 200; ++i)
    found = lfind(&i, arr, &nmemb, sizeof(int), cmpint);
}