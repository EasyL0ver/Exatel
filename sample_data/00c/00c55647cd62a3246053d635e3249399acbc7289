#include <stdio.h>

int min();

int main() {
    int array[5] = {5, 3, 5, 7, 1};
    printf("%d\n", min(array));
    return 0;

int min(int *feld) {
    int minimum = feld[0];
    for(int i = 1; i <= sizeof(feld); i++) {
        if(feld[i] < minimum) {
            minimum = feld[i];
        }
    }
    return minimum;
}