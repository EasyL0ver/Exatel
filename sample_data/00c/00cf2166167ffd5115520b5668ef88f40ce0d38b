function getParam($i)
{
    $text = getZoom()[$i];
    $parametre = explode("-", $text);
    $ordre = $parametre[0];
    $posX = $parametre[1];
    $posY = $parametre[2];
    $scale = $parametre[3];
    $interval = $parametre[4];
    $finZoom = $parametre[5];

    $tableauValeurs = array(
        "ordre" => $ordre,
        "posX" => $posX,
        "posY" => $posY,
        "scale" => $scale,
        "interval" => $interval,
        "finZoom" => $finZoom,
        );
    return $tableauValeurs;
};