public class Lotnisko {
    private List<Samolot> samoloty = new ArrayList<Samolot>();
    private List<Lot> loty = new ArrayList<Lot>();
    private String adresLotniska;
    
    public Lotnisko(String adresLotniska) {
        this.adresLotniska = adresLotniska;
    }
    
    public void stworzSamolot(String nazwa, int liczbaMiejsc) {
        this.samoloty.add(new Samolot(nazwa, this, liczbaMiejsc));
    }
    
 //gettery i setery
}