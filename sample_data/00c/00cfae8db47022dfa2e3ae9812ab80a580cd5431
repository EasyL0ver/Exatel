componentDidMount () {
  const { dispatch, match } = this.props
  dispatch(loadPipelineExecutions(match.params.pipelineId))
  this.reloadExecutions = setInterval(
    () => dispatch(loadPipelineExecutions(match.params.pipelineId)),
    3000
  )
}

componentWillUnmount () {
  clearInterval(this.reloadExecutions)
}