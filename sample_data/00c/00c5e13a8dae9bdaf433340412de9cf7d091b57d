
#include <iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int A[n], B[n];
    for(int i=0; i<=n; i+=2)
    {
        A[i]=i;
        cout<<"parni broevi se:"<<A[i]<<" ";
    }
    for(int j=1; j<=n; j+=2)
    {
      B[j]=j;
      cout<<"neparni broevi se:"<<B[j]<<" ";
    }
}