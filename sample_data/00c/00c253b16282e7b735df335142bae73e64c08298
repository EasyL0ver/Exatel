package net.embered.lobby.objects;

import org.bukkit.Material;
import org.bukkit.inventory.ItemStack;

/**
 * 
 * @author CubePixels | Niko
 *
 * Created on 24.06.2018
 *
 * https://www.spigotmc.org/members/cubepixels.543093/
 * Owner of Embered.net
 * 
 */
public class Gadget {
	
	private String name;
	private String displayName;
	private Material material;
	private ItemStack itemStack;
	private int subid;
	private int anzahl;
	private String data;
	
	public Gadget(String name, Material material, String displayName, Integer anzahl) {
		
		this.name = name;
		this.material = material;
		this.displayName = displayName;
		this.anzahl = anzahl;
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDisplayName() {
		return displayName;
	}

	public void setDisplayName(String displayName) {
		this.displayName = displayName;
	}

	public Material getMaterial() {
		return material;
	}

	public void setMaterial(Material material) {
		this.material = material;
	}

	public int getSubid() {
		return subid;
	}

	public void setSubid(int subid) {
		this.subid = subid;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public ItemStack getItemStack() {
		return itemStack;
	}

	public void setItemStack(ItemStack itemStack) {
		this.itemStack = itemStack;
	}
	
	public ItemStack toGadget() {
		return itemStack;
	}

	public int getAnzahl() {
		return anzahl;
	}

	public void setAnzahl(int anzahl) {
		this.anzahl = anzahl;
	}

}