#include <iostream>
#include <algorithm>
#include <vector>

using namespace std;

int main() {
    int n, x = 0, q, f;
    cin >> n;
    vector<int> b(n);
    for (int i = 0; i < n; ++i) {
        cin >> x;
        f = 0;
        for (int j = 0; j < q; ++j) {
            if (x == b[j]) {
                f++;
                break;
            }
        }
        if (f == 0) {
            b[q] = x;
            q++;
        }
    }
    sort(b.begin(), b.end());
    for (int i = 0; i < q; ++i) {
        cout << b[i] << ' ';
    }
    int i = 0, mx = 0;
    while (i <= q - 1){
        x = 1;
        if(b[i] == b[i + 1] - 1){
            while(b[i] == b[i + 1] - 1){
                i++;
                x++;
            }
            if(mx < x) mx = x;
        }
        else i++;
    }
    //cout << mx;
    return 0;
}