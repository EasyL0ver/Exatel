from docx import Document

wordDoc = Document('D:/dataforpython/FormsDoc.docx')

for table in wordDoc.tables:
    for row in (table.rows[1],table.rows[-1]):
        for cell in row.cells:
            print (cell.text)
	
from docx import Document

wordDoc = Document('D:/dataforpython/FormsDoc.docx')

for table in wordDoc.tables:
    for row in range(1,-1):
        for cell in row.cells:
        print (cell.text)
	
