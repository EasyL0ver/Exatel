local component = require("component")
local sides = require("sides")
local t = component.get("14b")
print(t)
print(component.invoke(t, "getInventoryName", sides.south))

local function searchItem(itemName) --returns the slot where the item is stored
	for i=0,component.invoke(component.get(t), "getInventorySize", sides.south, 1) do
		for _, _, size, maxSize, id,name, _, _ in component.invoke(component.get(t), "getStackInSlot", sides.south, i) do
			if (name == itemName and size >= 8) then
				return i
			end
		end
	end
end

local function fillAllSlotsInMixer(ing)
	for i=1,8,1 do
		component.invoke(component.get(t), "transferItem", sides.south, sides.west, 1, searchItem(ing), i)
	end
end

fillAllSlotsInMixer("redstone")