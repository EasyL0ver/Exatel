#include  <bits/stdc++.h>
using namespace std;
const int NMAX = 200010;
vector <int > ans;
struct vertex {
    int next[600];
    bool list1;
};
vertex t[NMAX];
int size1;
void add_string(string s) {
    int v = 0;
    for (int i =0; i < s.size(); i++) {
            char c = s[i] - 'a';
            if (t[v].next[c] == -1) {
                memset (t[size1].next, 255, sizeof t[size1].next);
                t[v].next[c] = size1;
                size1++;
            }
		v = t[v].next[c];
    }
    t[v].list1 = 1;
}
bool check1(string s) {
    int v = 0;
    for (int i =0; i < s.size(); i++) {
            char c = s[i] - 'a';
            if (t[v].next[c] == -1) return false;
            v = t[v].next[c];
    }
    return true;
}
int main() {
    size1 = 1;
    memset (t[0].next, 255, sizeof t[0].next);
    string s , s1;
    cin >> s;
    add_string(s);
    while (s.size() > 0) {
         s.erase(0 , 1);
          add_string(s);
    }
    int n;
    cin >> n;
    for (int i =0; i < n; i++) {
            cin >> s;
            if (check1(s)) cout << "Yes" << "\n";
            else
                cout << "No" << "\n";
    }
}