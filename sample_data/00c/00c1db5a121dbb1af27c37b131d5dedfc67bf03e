<script type="text/javascript">
$(document).ready(function() {
 $("#slideshow").css("overflow", "hidden");
 $("ul#slides").cycle({
    fx: 'fade' ,
    pause: 1
});


$("#slideshow").hover(function() {
   $("ul#nav").fadeIn();
},  // IF I REMOVE THIS COMMA THEN IT FAILS IN ALL BROWSERS.

    function() {
    $("ul#nav").fadeOut();
});
});
</script>