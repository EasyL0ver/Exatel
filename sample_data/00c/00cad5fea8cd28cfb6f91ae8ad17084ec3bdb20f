#include <iostream>
using namespace std;

bool isPrime(long long n){
  cin>>n;
  if(n<2) return false;
  else if(n==2) return true;
  else if(n==5) return true;
  else if(n%2!=0 && n%3!=0 && n%5!=0) {return true;}
  return false;
}

int main() {
  long long n;
  if(isPrime(n)==true) cout<<"1"<<endl;
  else cout<<"0"<<endl;
  return 0;
}
//pirminis 1