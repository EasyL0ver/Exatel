import java.util.Scanner;

public class Main {

    public static void main (String[] args) {

        Scanner input = new Scanner (System.in);

        System.out.println ("Wprowadź 3 liczby.");

        System.out.println ("Pierwsza liczba: ");

        int value1 = input.nextInt();

        System.out.println ("Druga liczba: ");

        int value2 = input.nextInt ();

        System.out.println ("Trzecia liczba: ");

        int value3 = input.nextInt ();

        if (value1 >= value2) {

            if (value1 >= value3) {

                System.out.println ("Największa wartość to: " + value1);

            }

        } else {

            if (value2 > value1) {

                if (value2 >= value3) {

                    System.out.println ("Największa wartość to: " + value2);

                } else {

                    if (value3 > value1) {

                        if (value3 > value2) {

                            System.out.println ("Największa wartość to: " + value3);

                        }

                    }

                }

            }

        }

    }

}