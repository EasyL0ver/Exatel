#include <stdio.h>

int Podniz (const int *p1, const int *p2, const int *q1, const int *q2) {
	
	int index=0;
	
	while(p1<p2) {
		if(*p1==*q1 && (p2-p1)>=(q2-q1)) {
			const int *privremena_p1=p1;
			const int *privremena_q1=q1;
			int istina=1;
			while(privremena_q1<q2) {
				if(*privremena_q1!=*privremena_p1) {
					istina=0;
					break;
				}
				privremena_p1++;
				privremena_q1++;
			}
			if(istina) return index;
		}
		p1++;
		index++;
	} 
	return -1;
}

int main() {
	printf("ZSR 8, Zadatak 7");
	return 0;
}