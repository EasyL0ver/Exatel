  s3.Pitch = 0.85
  s3.Volume = 1
  s3.Looped = false
  s3:Play()
  xay = Instance.new("Sound", workspace)
  xay.SoundId = "rbxassetid://419447292"
  xay.Pitch = 1
  xay.Volume = 3.5
  xay:Play()
  expld.BlastRadius = rad
  expld.Position = par.Position
  partMesh.Scale = vt(rad, rad, rad)
  expart.Size = vt(1, 1, 1) * 1.5
 